- if layout
  - extends "ekklesia_portal/layout.j2.jade"

- block title
  title
    = brand_title
    |&nbsp;-&nbsp;
    = _("overview")

- block content
  if search
    ol.breadcrumb
      li
        a(href=self_link)
          = _("overview")
      li.active
        = _("search_for")
        |:
        strong= search

  if tag
    ol.breadcrumb
      li
        a(href=self_link)
          = _("overview")
      li.active
        | Nur Antr√§ge mit Tag:
        span.tag_text= tag

  = render_cell(collection=propositions)


